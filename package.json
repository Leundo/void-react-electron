{
  "name": "void-react-electron",
  "productName": "Void React Electron",
  "description": "Starter for an Electron application including React.",
  "version": "0.0.0",
  "private": true,
  "author": "Leundo <leundo@example.com>",
  "copyright": "Â© 2021, Leundo.",
  "homepage": ".",
  "main": "build/electron.js",
  "build": {
    "appId": "com.example.void-react-electron",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  },
  "scripts": {
    "elec-dev": "electron ./src/main.js dev",
    "elec": "electron ./src/main.js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "release": "webpack --config=config/webpack/webpack.app.config.js --env=production && electron-builder"
  },
  "dependencies": {
    "fs-jetpack": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "chai": "^4.1.0",
    "css-loader": "^5.2.0",
    "electron": "^15.0.1",
    "electron-builder": "^22.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3",
    "source-map-support": "^0.5.6",
    "style-loader": "^2.0.0",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
